    "npm-run-all": "^4.1.5",
    "rollup": "^2.26.x",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-scss": "^2.6.0",
    "rollup-plugin-svelte": "6.x",
    "rollup-plugin-terser": "7.x",
    "sass": "^1.26.10",
    "svelte": "^3.24.1"
  },
  "dependencies": {
    "sirv-cli": "^1.0.3",
